#include <iostream>

using namespace std;

int main()
{
    double x, y, z, zsum=144.81;

    cout << "Kiek kainuoja vyriskos kelnes?: "; cin >> x;
    cout << "Kiek kainuoja svarkas?: "; cin >> y;
    cout << "Kiek kainuoja medziaga  vyriskam kostiumui pasiuti?: "; cin >> z;
    cout << "!!|__Pasiuvimas__|!! " << zsum << " Eur" << endl;

    if ((x+y)<(z+zsum))
        cout << "Pirkti parduotuveje yra pigiau" << endl;
    else if ((x+y)>(z+zsum))
        cout << "Siuti kostiuma yra pigiau" << endl;


    return 0;
}
