#include <iostream>

using namespace std;

int main()
{
    double x, y, z;

    cout << "Kiek kainuoja auskarai?: "; cin >> x;
    cout << "Kiek kainuoja apyranke?: "; cin >> y;
    cout << "Kiek kainuoja riesines?: "; cin >> z;

    if (x>y && x>z)
        cout << "Isleis " << x << " Eur" << endl;
    else if (y>x && y>z)
        cout << "Isleis " << y << " Eur" << endl;
    else if (z>y && z>x)
        cout << "Isleis " << z << " eur" << endl;
    return 0;
}
