#include <iostream>

using namespace std;

int main()
{
    double x, y, z;

    cout << "Kiek kainuoja knyga?: "; cin >> x;
    cout << "Kiek kainuoja megstamos grupes kompaktine plokstele?: "; cin >> y;
    cout << "Kiek kainuoja atmintukas?: "; cin >> z;

    if (x<y && x<z)
        cout << "Isleis " << x << " Eur" << endl;
    else if (y<x && y<z)
        cout << "Isleis " << y << " Eur" << endl;
    else if (z<y && z<x)
        cout << "Isleis " << z << " eur" << endl;
    return 0;
